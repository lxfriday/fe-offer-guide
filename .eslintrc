{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "linebreak-style": 0,
    "semi": 0,
    "no-param-reassign": 0,
    "no-extra-semi": 0,
    "no-plusplus": 0,
    "no-unused-vars": 1,
    "prefer-const": 1,
    "no-restricted-syntax": 0,
    "no-continue": 0
  }
}
